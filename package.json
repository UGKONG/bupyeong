{
  "name": "bu",
  "version": "1.0.0",
  "description": "부평 스마트 커뮤니티 플랫폼",
  "main": "index.js",
  "keywords": [
    "부평",
    "부평 스마트 커뮤니티 플랫폼",
    "부평 커뮤니티"
  ],
  "author": "",
  "license": "ISC",
  "scripts": {
    "start": "webpack-dev-server --mode development --hot --port 8081",
    "build": "webpack --mode production",
    "watch": "webpack -w",
    "db": "json-server --watch --host localhost --port 8082 ./db.json",
    "db:12": "json-server --watch --host 192.168.0.12 --port 8082 ./db.json",
    "db:59": "json-server --watch --host 192.168.0.59 --port 8082 ./db.json",
    "db:90": "json-server --watch --host 192.168.0.90 --port 8082 ./db.json",
    "db2": "json-server --watch --host localhost --port 8083 ./db2.json",
    "db2:12": "json-server --watch --host 192.168.0.12 --port 8083 ./db2.json",
    "db2:59": "json-server --watch --host 192.168.0.59 --port 8083 ./db2.json",
    "db2:90": "json-server --watch --host 192.168.0.90 --port 8083 ./db2.json",
    "dev": "concurrently \"npm run db\" \"npm run db2\" \"npm run start\"",
    "dev:12": "concurrently \"npm run db:12\" \"npm run db2:12\" \"npm run start\"",
    "dev:59": "concurrently \"npm run db:59\" \"npm run db2:59\" \"npm run start\"",
    "dev:90": "concurrently \"npm run db:90\" \"npm run db2:90\" \"npm run start\"",
    "test": "concurrently \"npm run start\" \"npm run watch\""
  },
  "dependencies": {
    "axios": "^0.24.0",
    "colorette": "^2.0.19",
    "jquery": "^3.6.0",
    "json-server": "^0.17.0",
    "prettier": "^2.7.1",
    "vue": "^2.6.14",
    "vue-axios-cors": "^1.0.1",
    "vue-qrcode-component": "^2.1.1",
    "vue-qrcode-reader": "^3.1.0",
    "vue-router": "^3.5.3",
    "vuedraggable": "^2.24.3",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.5.1",
    "file-loader": "^6.2.0",
    "html-loader": "^3.0.1",
    "html-webpack-plugin": "^5.5.0",
    "sass": "^1.53.0",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "vue-loader": "^15.9.8",
    "vue-sortable": "^0.1.3",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.64.2",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3"
  }
}
